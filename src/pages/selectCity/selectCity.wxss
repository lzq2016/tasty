/* pages/selectCity/selectCity.wxss */

.box {
  width: 400rpx;
  padding: 100rpx 50rpx;
  margin: 0 auto;
  border: 10rpx solid #fff;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-left: -200rpx;
  z-index: 10001;
}



.cityPicker {
  height: 50px;
  border-top: 1rpx solid #fff;
  border-bottom: 1rpx solid #fff;
}

.layer {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-image: url('http://lzqrebate.oss-cn-beijing.aliyuncs.com/homebackground.png');
  background-position: cover;
  z-index: 10000;
}

.citypickerWrap {
  background: transparent;
  width: 400rpx;
  height: 150px;
}
.cityPickerMask{
  background-color: transparent;
}
picker-view-column{
  background-color: transparent;
}
